{
    "name": "origin-oss/laravel-openapi-demo",
    "description": "Demo application for the Laravel OpenAPI package.",
    "type": "project",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0 || ^11.0",
        "origin-oss/laravel-openapi": "@dev"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../",
            "options": {
                "symlink": true
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
       "post-autoload-dump": [
           "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
           "@php artisan package:discover --ansi"
       ]
   }
}
