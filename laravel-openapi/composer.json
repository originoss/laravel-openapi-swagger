{
    "name": "origin-oss/laravel-openapi",
    "description": "A Laravel package for generating OpenAPI specifications from PHP 8 attributes.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Origin OSS",
            "email": "hello@originoss.org"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0",
        "illuminate/console": "^9.0 || ^10.0 || ^11.0",
        "illuminate/routing": "^9.0 || ^10.0 || ^11.0",
        "illuminate/filesystem": "^9.0 || ^10.0 || ^11.0",
        "symfony/finder": "^5.4 || ^6.0 || ^7.0",
        "symfony/yaml": "^5.4 || ^6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelOpenApi\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelOpenApi\\LaravelOpenApiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
